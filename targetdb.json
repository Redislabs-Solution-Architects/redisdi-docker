{
    "name": "targetdb",
    "type": "redis",
    "memory_size":  100000000,
    "port": 12000,
    "proxy_policy": "all-nodes",
    "sharding": true,
    "shards_count": 1,
    "shards_placement": "sparse",
    "shard_key_regex": [{"regex": ".*\\{(?<tag>.*)\\}.*"}, {"regex": "(?<tag>.*)"}],
    "replication": false,
    "authentication_redis_pass": "redis",
    "module_list": [{
        "module_name":"ReJSON",
        "module_args": "",
        "semantic_version": "2.4.3"
    }]
}